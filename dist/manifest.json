{
  "manifest_version": 3,
  "name": "Google SERP Numerator",
  "version": "0.2.0",
  "description": "Numbers organic Google results and labels ads + notes.",
  "permissions": ["storage"],
  "content_scripts": [
    {
      "matches": ["*://*.google.com/search*", "*://*.google.pl/search*"],
      "js": ["contentScript.js"],
      "css": [],
      "run_at": "document_idle"
    }
  ]
}
